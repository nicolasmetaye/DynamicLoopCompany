@using DynamicLoopCompany.Components.Models.Master
@using AutoMapper
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var metaTagsModel = Mapper.Map<IPublishedContent, MetaTagsModel>(Model.Content);
    var mainMenuModel = Mapper.Map<IPublishedContent, MainMenuModel>(Model.Content);
    var footerModel = Mapper.Map<IPublishedContent, FooterLinksModel>(Model.Content);
}
<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->
<head>
	<title>@Model.Content.GetPropertyValue("title")</title>
	@Html.Partial("MetaTags",metaTagsModel)
    <meta http-equiv="Content-Language" content="en" />
    <meta name="language" content="en" />
    <meta charset="UTF-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssreset/cssreset-min.css" />
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssbase/cssbase-min.css" />
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssfonts/cssfonts-min.css" />
    <link rel="stylesheet" type="text/css" href="/css/styles.css" />
    <script src="/scripts/modernizr-2.6.2.min.js" type="text/javascript"></script>
    <script src="/scripts/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script src="/scripts/jquery-ui.js"></script>
    <script src="/scripts/menu.js" type="text/javascript"></script>
    <script src="/scripts/disclaimer.js" type="text/javascript"></script>
    @RenderSection("Header", false)
</head>
<body>
    <div id="mainContainer">
        <div id="background">
            <div id="backgroundTop">
                <div id="header">
                    <div class="company-name-container">
                        <h1 class="company-name">Company Name</h1>
                    </div>
                    @Html.Partial("MainMenu", mainMenuModel)
                </div>
                <div id="mainContent">
                    @RenderBody()
                </div>
            </div>
        </div>
        @Html.Partial("Footer", footerModel)
    </div>
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.9.1.js"></script>
</body>
</html>